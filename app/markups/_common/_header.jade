header.header
	.header__content
		header.header__content__upper
			a.header__link(href="index.html")
				span.header__link__text Транспортное строительство
				img.header__logo(alt="header-logo" src="img/content/logo/logo_header.png")
			-headerTextUpper = 'Транспортное строительство'
			-headerTextBottom = 'Автоматика Телемеханика Связь'
			h1.header__text 
				.header__text_upper= headerTextUpper.toUpperCase()
				.header__text_bottom= headerTextBottom.toUpperCase()
		header.header__content__bottom
			-mainNavItems = {'Главная' : '#', 'Наши проекты' : '#', 'О компании' : '#', 'Контакты' : '#'}
			+nav('header__main-nav', mainNavItems, true)
			-subMenuItems = {'2000' : '#', '2001' : '#', '2002' : '#', '2003' : '#', '2004' : '#', '2005' : '#', '2006' : '#', '2007' : '#', '2008' : '#', '2009' : '#', '2010' : '#', '2011' : '#', '2012' : '#', '2013' : '#', '2014' : '#', '2015' : '#'};
			+nav('header__sub-nav', subMenuItems, true)



